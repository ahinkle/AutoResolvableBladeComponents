{
    "name": "ahinkle/auto-resolvable-blade-components",
    "description": "A Laravel package to automatically resolve your Blade Component views based on the component class names.",
    "keywords": [
        "ahinkle",
        "auto",
        "blade",
        "resolvable",
        "views",
        "laravel",
        "components",
        "component"
    ],
    "homepage": "https://github.com/ahinkle/laravel-resolvable-components",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andy Hinkle",
            "email": "ahinkle10@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/view": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^9.5|^10.0",
        "illuminate/container": "^10.0",
        "illuminate/contracts": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ahinkle\\AutoResolvableComponents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ahinkle\\AutoResolvableComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ahinkle\\AutoResolvableComponents\\AutoResolvableComponentsServiceProvider"
            ]
        }
    }
}
